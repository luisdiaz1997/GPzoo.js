{
  "name": "gpzoo",
  "version": "0.1.0",
  "description": "Gaussian Process library for JavaScript/TypeScript",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "bun build ./src/index.ts --outdir ./dist --target node --format esm && bun build ./src/index.ts --outfile ./dist/index.cjs --target node --format cjs && bun build ./src/index.ts --outfile ./dist/index.js --target browser --format umd --name gpzoo",
    "test": "bun test",
    "lint": "bunx @biomejs/biome check src/"
  },
  "keywords": [
    "gaussian-process",
    "machine-learning",
    "statistics"
  ],
  "license": "MIT",
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "bun-types": "latest"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  }
}
